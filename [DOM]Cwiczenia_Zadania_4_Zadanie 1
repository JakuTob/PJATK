#include <iostream>
using namespace std;

bool CzySzescian(int A)
{
	for (int i = 0; i * i * i <= A; i++)
		if (i * i * i == A)	return true;
		return false;
}
int main()
{
	int A;
	cin >> A;
	if (CzySzescian(A))
	{
		cout << "1";
	}
	else
	{
		cout << "0";
	}
	return 0;
}

// Cwiczenia_Zadania_4_Zadanie 1
